{
  "name": "makesense",
  "displayName": "makesense",
  "description": "Makefile Prompt Complement Jump Help",
  "version": "0.0.1",
  "publisher": "kamilu",
  "repository": {
    "type": "git",
    "url": "https://github.com/luzhixing12345/makesense"
  },
  "bugs": {
    "email": "luzhixing12345@163.com",
    "url": "https://github.com/luzhixing12345/makesense/issues"
  },
  "homepage": "https://github.com/luzhixing12345/makesense",
  "keywords": [
    "Makefile",
    "makefile",
    "MAKEFILE"
  ],
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:makesense.triggerSuggest"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "makesense language",
      "properties": {
        "language": {
          "type": "string",
          "enum": [
            "en",
            "zh"
          ],
          "default": "zh",
          "description": "The language used by the extension"
        }
      }
    },
    "snippets": [
      {
        "language": "makefile",
        "path": "./makefile_snippets.json"
      }
    ],
    "commands": [
      {
        "command": "makesense.createMakefile",
        "title": "Create Makefile"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerViewletFocus",
          "command": "makesense.createMakefile"
        }
      ]
    },
    "languages": [
      {
        "id": "makefile",
        "extensions": [
          ".mk",
          ".mak"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "semanticLanguage",
        "extensions": [
            ".semanticLanguage"
        ]
    }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}
